{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Memory",
  "type": "object",
  "required": [
    "id",
    "name",
    "image",
    "rarity"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "rarity": {
      "type": "string",
      "enum": [
        "Common",
        "Uncommon",
        "Rare",
        "Holo"
      ]
    },
    "set": {
      "type": [
        "string",
        "null"
      ]
    },
    "lore": {
      "type": [
        "string",
        "null"
      ]
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}