{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Venue",
  "type": "object",
  "required": [
    "id",
    "name",
    "map",
    "pins",
    "faqs"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "map": {
      "type": "string"
    },
    "pins": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "x": {
            "type": "number"
          },
          "y": {
            "type": "number"
          }
        },
        "required": [
          "label",
          "x",
          "y"
        ]
      }
    },
    "faqs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "q": {
            "type": "string"
          },
          "a": {
            "type": "string"
          }
        },
        "required": [
          "q",
          "a"
        ]
      }
    }
  },
  "additionalProperties": false
}