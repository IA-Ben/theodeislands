    "id",
    "title",
    "image",
    "price"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "variants": {
      "type": "array",
